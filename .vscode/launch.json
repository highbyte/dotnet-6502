{
   // Use IntelliSense to find out which attributes exist for C# debugging
   // Use hover for the description of the existing attributes
   // For further information visit https://github.com/OmniSharp/omnisharp-vscode/blob/master/debugger-launchjson.md
   "version": "0.2.0",
   "configurations": [
        {
            "name": ".NET Core Launch (console monitor)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build monitor",
            // If you have changed target frameworks, make sure to update the program path.
            "program": "${workspaceFolder}/Highbyte.DotNet6502.App/Highbyte.DotNet6502.App.Monitor/bin/Debug/net6.0/Highbyte.DotNet6502.App.Monitor.dll",
            "args": [],
            "cwd": "${workspaceFolder}/Highbyte.DotNet6502.App/Highbyte.DotNet6502.App.Monitor/bin/Debug/net6.0",
            // For more information about the 'console' field, see https://aka.ms/VSCode-CS-LaunchJson-Console
            "console": "externalTerminal",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Launch (console test)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build console test",
            // If you have changed target frameworks, make sure to update the program path.
            "program": "${workspaceFolder}/Examples/ConsoleTestPrograms/bin/Debug/net6.0/ConsoleTestPrograms.dll",
            "args": [],
            "cwd": "${workspaceFolder}/Examples/ConsoleTestPrograms/bin/Debug/net6.0",
            // For more information about the 'console' field, see https://aka.ms/VSCode-CS-LaunchJson-Console
            "console": "externalTerminal",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Launch (sadconsole test)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build sadconsole test",
            // If you have changed target frameworks, make sure to update the program path.
            "program": "${workspaceFolder}/Examples/SadConsoleTest/bin/Debug/net6.0/SadConsoleTest.dll",
            "args": [],
            "cwd": "${workspaceFolder}/Examples/SadConsoleTest/bin/Debug/net6.0",
            // For more information about the 'console' field, see https://aka.ms/VSCode-CS-LaunchJson-Console
            "console": "integratedTerminal",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Launch (SkiaNative)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build skia native app",
            // If you have changed target frameworks, make sure to update the program path.
            "program": "${workspaceFolder}/Highbyte.DotNet6502.App/Highbyte.DotNet6502.App.SkiaNative/bin/Debug/net6.0/Highbyte.DotNet6502.App.SkiaNative.dll",
            "args": [],
            "cwd": "${workspaceFolder}/Highbyte.DotNet6502.App/Highbyte.DotNet6502.App.SkiaNative/bin/Debug/net6.0",
            // For more information about the 'console' field, see https://aka.ms/VSCode-CS-LaunchJson-Console
            "console": "integratedTerminal",
            "stopAtEntry": false
        },        
        {
            "name": "Blazor WASM Launch and Debug (BlazorWasmTest)",
            "type": "blazorwasm",
            "request": "launch",
            "cwd": "${workspaceFolder}/Examples/BlazorWasmTest",
            "url": "http://localhost:5000",
            "browser": "chrome"
        },
        {
            "name": "Blazor WASM Skia Launch and Debug (BlazorWasmSkiaTest)",
            "type": "blazorwasm",
            "request": "launch",
            "cwd": "${workspaceFolder}/Examples/BlazorWasmSkiaTest",
            "url": "http://localhost:5000",
            "browser": "chrome"
        },        
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}